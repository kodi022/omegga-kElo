{
  "name": "k-Elo",
  "description": "A typed safe Omegga plugin.",
  "author": "Kodi022",
  "config": {
    "Blocked-Minis": {
      "description": "If in one of these minis and not auth, will be forced back into spec mini",
      "type": "list",
      "itemType": "string",
      "default": ["GLOBAL"]
    },
    "Authorized-Users": {
      "description": "authorized users",
      "type": "players",
      "default": []
    },
    "Print-Kills": {
      "description": "Enable same-minigame limited kill prints in chat (uses a some cpu)",
      "type": "boolean",
      "default": false
    },
    "Enable-Debug": {
      "description": "Enables debug broadcasts",
      "type": "boolean",
      "default": false
    }
  },
  "commands": []
}
